{
  "name": "photo-editor",
  "version": "0.2.0",
  "description": "Simple and customizable photo editor for web applications.",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "typings": "ts/index.d.ts",
  "files": [
    "/es",
    "/lib",
    "/tools",
    "/ts"
  ],
  "scripts": {
    "clean": "rimraf es lib ts",
    "build": "yarn build:cjs && yarn build:es && yarn build:ts",
    "build:cjs": "cross-env BABEL_ENV=cjs babel --root-mode upward --extensions '.ts,.tsx' src --out-dir lib --ignore \"src/**/__tests__\"",
    "build:es": "cross-env BABEL_ENV=es babel --root-mode upward --extensions '.ts,.tsx' src --out-dir es --ignore \"src/**/__tests__\"",
    "build:ts": "tsc --declaration --emitDeclarationOnly",
    "test": "yarn lint && yarn test:unit",
    "lint": "eslint src --ext .ts,.tsx --ignore-path ../../.gitignore",
    "test:ts": "tsc -p ./tsconfig.validate.json",
    "test:unit": "jest",
    "prepare": "yarn clean && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vtaits/photo-editor-js.git"
  },
  "keywords": [
    "editor",
    "image",
    "photo",
    "vanilla"
  ],
  "author": "Vadim Taits",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vtaits/photo-editor-js/issues"
  },
  "homepage": "https://github.com/vtaits/photo-editor-js/tree/master/packages/photo-editor#readme",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/eslint-parser": "^7.15.7",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.5.0",
    "jest": "^27.2.4",
    "jest-canvas-mock": "^2.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "eventemitter3": "^4.0.7"
  }
}
